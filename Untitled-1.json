        // Initialize Three.js for preview
        function initPreviewThreeJS() {
            const container = document.getElementById('preview-canvas-container');
            
            // Clear container
            container.innerHTML = '';
            
            const previewScene = new THREE.Scene();
            const previewCamera = new THREE.PerspectiveCamera(75, container.clientWidth / container.clientHeight, 0.1, 1000);
            const previewRenderer = new THREE.WebGLRenderer({ alpha: true, antialias: true });
            previewRenderer.setSize(container.clientWidth, container.clientHeight);
            container.appendChild(previewRenderer.domElement);
            
            // Create a rotating cube
            const geometry = new THREE.BoxGeometry(1, 1, 1);
            const material = new THREE.MeshPhongMaterial({ 
                color: 0xbd00ff,
                emissive: 0xbd00ff,
                emissiveIntensity: 0.3
            });
            const cube = new THREE.Mesh(geometry, material);
            previewScene.add(cube);
            
            // Add wireframe
            const wireframe = new THREE.WireframeGeometry(geometry);
            const line = new THREE.LineSegments(wireframe);
            line.material.color.set(0x00f3ff);
            line.material.opacity = 0.5;
            line.material.transparent = true;
            cube.add(line);
            
            previewCamera.position.z = 3;
            
            // Add lights
            const ambientLight = new THREE.AmbientLight(0xffffff, 0.5);
            previewScene.add(ambientLight);
            
            const directionalLight = new THREE.DirectionalLight(0x00f3ff, 1);
            directionalLight.position.set(5, 5, 5);
            previewScene.add(directionalLight);
            
            // Animation loop
            function animate() {
                requestAnimationFrame(animate);
                
                if (cube) {
                    cube.rotation.x += 0.02;
                    cube.rotation.y += 0.02;
                }
                
                previewRenderer.render(previewScene, previewCamera);
            }
            
            animate();
            
            // Handle window resize
            window.addEventListener('resize', () => {
                previewCamera.aspect = container.clientWidth / container.clientHeight;
                previewCamera.updateProjectionMatrix();
                previewRenderer.setSize(container.clientWidth, container.clientHeight);
            });
        }
        
